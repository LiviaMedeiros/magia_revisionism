                "voiceFull": "fullvoice/section_101942/vo_full_101942-3-10",
                "voiceFullStop": "fullvoice/section_101942/vo_full_101942-3-9"
            },
            {
                "narration": "そして今、彼女たちは@神浜の町に手を出そうとしています",
                "voiceFull": "fullvoice/section_101942/vo_full_101942-3-11",
                "voiceFullStop": "fullvoice/section_101942/vo_full_101942-3-10"
