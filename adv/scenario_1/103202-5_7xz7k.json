            },
            {
                "nameNarration": "十七夜",
                "narration": "―十七夜―@共に繁華街で生き抜いた友は恐らく@どこかで潰されてしまった",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-2"
            },
            {
                "nameNarration": "みたま",
                "narration": "―みたま―@劇団の仲間たちは死して屍となった",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-3"
            },
            {
                "nameNarration": "十七夜",
                "narration": "―十七夜―@さらわれる前に過ごした家族の記憶とともに",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-4"
            },
            {
                "nameNarration": "みたま",
                "narration": "―みたま―@裏社会に潰された記憶とともに",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-5"
            },
            {
                "nameNarration": "十七夜",
                "narration": "―十七夜―@紛争に翻弄された記憶とともに",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-6"
            },
            {
                "nameNarration": "みたま",
                "narration": "―みたま―@民族のぶつかり合いで見た血の記憶とともに…",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-7"
            },
            {
                "nameNarration": "十七夜",
                "narration": "―十七夜―@世界の不幸を俯瞰して見た彼女は@自分の境遇に飲まれずに生きようとした",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-8"
            },
            {
                "nameNarration": "みたま",
                "narration": "―みたま―@だけど善人が生きる世界に戻った先生は@前以上に生きづらい時を過ごすことになった",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-9"
            },
            {
                "nameNarration": "十七夜",
                "narration": "―十七夜―@それでも悲しみを内に溜めて憎悪とせずに@糧にして冷静に解決しようとしている",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-10"
            },
            {
                "nameNarration": "みたま",
                "narration": "―みたま―@先生は糧にしろと言うのですか@堕ちた自分たちを新たにするために",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-11"
            },
            {
                "autoTurnLast": 0.2,
                "narrationEffect": "out"
            },
            {
                "nameNarration": "リヴィアの声",
                "narration": "「そういうこっちゃ」",
                "narrationEffect": "in",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-12"
            },
            {
                "narration": "「どんな味付けにしても構わへんから@はよう可哀相やと思とる自分を引きずり出して@そのまま食ってしもうたれ」",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-13"
            },
            {
                "autoTurnLast": 0.2,
                "narrationEffect": "out"
            },
            {
                "nameNarration": "みたま",
                "narration": "―みたま―@でも、どれだけ俯瞰しても、@神浜市を滅ぼすわたしは消えない",
                "narrationAnchor": "bottom",
                "narrationCoverOpacity": 110,
                "narrationEffect": "in",
                "narrationFontSize": 30,
                "narrationLineMargin": 10,
                "narrationLineSpace": 20,
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-14"
            },
            {
                "narration": "―みたま―@それに、この悲しみを食らったら@わたし自身を失うのと同じよ",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-15"
            },
            {
                "nameNarration": "十七夜",
                "narration": "―十七夜―@…確かに、そうかもしれんな",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-16"
            },
            {
                "bgm": "stop",
                "narration": "―十七夜―@しかし八雲@存外、そうではないかもしれん",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-17"
            },
            {
                "nameNarration": "みたま",
                "narration": "―みたま―@十七夜…？",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-18"
            },
            {
                "bgm": "bgm05_event06",
                "nameNarration": "十七夜",
                "narration": "―十七夜―@我々は、自分の中にある悲しみに@満たされた神浜市を滅ぼすんだ",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-19"
            },
            {
                "narration": "―十七夜―@そして次は、共に歩みたい@神浜市と共に生きる",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-20"
            },
            {
                "nameNarration": "みたま",
                "narration": "―みたま―@ミィたちが繋いでくれた@神浜市と一緒に…？",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-21"
            },
            {
                "nameNarration": "十七夜",
                "narration": "―十七夜―@そう、そのためにも我々は、@古い我々を食らわなくてはならん",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-22"
            },
            {
                "nameNarration": "みたま",
                "narration": "―みたま―@…そして自分たちの糧にするのね",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-23"
            },
            {
                "nameNarration": "十七夜",
                "narration": "―十七夜―@うむ、未来のためにも@悲しみに囚われるのはここまでだ",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-24"
            },
            {
                "nameNarration": "みたま",
                "narration": "―みたま―@…わかったわ",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-25"
            },
            {
                "narration": "―みたま―@ミィが繋いでくれた明日のために@わたしも過去を食らうわ",
                "voiceFullAuto": "fullvoice/section_103202/vo_full_103202-5-26"
